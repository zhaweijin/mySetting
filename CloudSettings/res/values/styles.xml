<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="FullscreenTheme" parent="android:Theme.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@null</item>
        <item name="buttonBarStyle">@style/ButtonBar</item>
        <item name="buttonBarButtonStyle">@style/ButtonBarButton</item>
    </style>

    <style name="ButtonBar">
        <item name="android:paddingLeft">2dp</item>
        <item name="android:paddingTop">5dp</item>
        <item name="android:paddingRight">2dp</item>
        <item name="android:paddingBottom">0dp</item>
        <item name="android:background">@android:drawable/bottom_bar</item>
    </style>

    <style name="layout_cycle_focus">
        <item name="android:layout_width">280dp</item>
        <item name="android:layout_height">280dp</item>
        <item name="android:background">@drawable/cycle_style</item>
        <item name="android:focusable">true</item>
    </style>
    <style name="layout_cycle_focus1">
        <item name="android:layout_width">280dp</item>
        <item name="android:layout_height">280dp</item>
        
        <item name="android:focusable">true</item>
    </style>
    
    <style name="layout_square_focus1">
        <item name="android:layout_width">280dp</item>
        <item name="android:layout_height">307dp</item>
        
        <item name="android:focusable">true</item>
    </style>

    <style name="layout_cycle_focus_p">
        <item name="android:src">@drawable/cycle_style</item>
    </style>

    <style name="noTitle">
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="myTranslucent">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
        <item name="android:windowNoTitle">true</item>
         <item name="android:windowFullscreen">true</item>
    </style>

    <style name="ButtonBarButton" />

    <style name="BasicDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:layout_width">@dimen/dialog_width</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowSoftInputMode">stateUnspecified|adjustPan</item>
    </style>

    <style name="CustomProgressDialog" parent="@style/BasicDialog">
        <item name="android:windowBackground">@drawable/dialog_bg</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="CustomProgressNewDialog" parent="@style/BasicDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="BluetoothPairDialog" parent="@style/BasicDialog">
        
        <item name="android:windowNoTitle">true</item>
    </style>
    

  <style name="menu_popu_in_out_style">  
        <item name="android:windowEnterAnimation">@anim/menu_popuwindow_in_anim</item>  
        <item name="android:windowExitAnimation">@anim/menu_popuwindow_out_anim</item>  
    </style> 
  
 
    <style name="domy_display_outputmode_text">
        <item name="android:textSize">@dimen/s_22</item>
        <item name="android:textColor">#c7c7c7</item>
    </style>
    
     
     
    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item> <!-- 边框 -->
        <item name="android:windowIsFloating">true</item> <!-- 是否浮现在activity之上 -->
        <item name="android:windowIsTranslucent">false</item> <!-- 半透明 -->
        <item name="android:windowNoTitle">true</item> <!-- 无标题 -->
        <item name="android:windowBackground">@color/trans</item> <!-- 背景透明 -->
        <item name="android:backgroundDimEnabled">true</item> <!-- 模糊 -->
    </style>
    
     
         <style name="domy_display_outputmode_list_select_text">
        <item name="android:textSize">@dimen/s_30</item>
        <item name="android:textColor">#636262</item>
    </style>

    <style name="domy_diaplay_set_title_text">
        <item name="android:textSize">@dimen/s_55</item>
        <item name="android:textColor">#F1F1F1</item>
    </style>

    <style name="domy_display_scop_button_text">
        <item name="android:textSize">@dimen/s_24</item>
        <item name="android:textColor">#919191</item>
    </style>

    <style name="domy_setting_position_message">
        <item name="android:textSize">@dimen/s_24</item>
        <item name="android:textColor">#5c5c5c</item>
    </style>

    <style name="domy_setting_position_ok_button_text">
        <item name="android:textSize">@dimen/s_24</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white_smoke</item>
    </style>

    <style name="domy_setting_position_button_text">
        <item name="android:textSize">@dimen/s_30</item>
        <item name="android:textColor">@color/dim_gray_2</item>
    </style>

    <style name="domy_setting_scop_text">
        <item name="android:textSize">@dimen/s_36</item>
    </style>

    <style name="domy_test_network_speed_text">
        <item name="android:textColor">@color/white_smoke</item>
        <item name="android:textSize">@dimen/s_24</item>
    </style>
    
    
    <style name="btn_0">
        <item name="android:focusable">true</item>
        <item name="android:background">@drawable/btn_style1</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="keyboard_layout">
        <item name="android:layout_width">110dp</item>
        <item name="android:layout_height">76dp</item>
        <item name="android:background">@drawable/keyboard_zimu</item>
        <item name="android:gravity">center</item>
        <item name="android:focusable">true</item>
    </style>

    <style name="keyboard_layout_zimu" parent="keyboard_layout">
        <item name="android:background">@drawable/keyboard_zimu</item>
    </style>

    <style name="keyboard_layout_finish" parent="keyboard_layout">
        <item name="android:background">@drawable/keyboard_finish</item>
    </style>

    <style name="keyboard_layout_delete" parent="keyboard_layout">
        <item name="android:background">@drawable/keyboard_delete</item>
    </style>

    <style name="keyboard_layout_num" parent="keyboard_layout">
        <item name="android:background">@drawable/keyboard_num</item>
    </style>

    <style name="keyboard_text">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:background">@drawable/key_zimu</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">25sp</item>
        <item name="android:textColor">@color/grey2_color</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="keyboard_text_zimu" parent="keyboard_text">
        <item name="android:background">@drawable/key_zimu</item>
    </style>

    <style name="keyboard_text_finish" parent="keyboard_text">
        <item name="android:background">@drawable/key_finish_normal</item>
    </style>

    <style name="keyboard_text_delete" parent="keyboard_text">
        <item name="android:background">@drawable/key_delete_normal</item>
    </style>

    <style name="keyboard_text_num" parent="keyboard_text">
        <item name="android:background">@drawable/key_num_normal</item>
    </style>

    <style name="keyboard_item">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/btn_0</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">25sp</item>
        <item name="android:textColor">@color/grey2_color</item>
        <item name="android:focusable">true</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="keyboard_item_1">
        <item name="android:layout_width">90dip</item>
        <item name="android:layout_height">55dip</item>
        <item name="android:background">@drawable/btn_0_board</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">25sp</item>
        <item name="android:textColor">@color/grey2_color</item>
        <item name="android:singleLine">true</item>
    </style>

    <style name="layout_focus_1">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@drawable/btn_style1</item>
        <item name="android:focusable">true</item>
    </style>

    <style name="Theme.TranslucentDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/board_s</item>
        <item name="android:colorForeground">#fff</item>
    </style>
    
    
    <style name="ActivityTitle">
        <item name="android:layout_marginLeft">50dp</item>
        <item name="android:layout_marginTop">40dp</item>
        <item name="android:textSize">@dimen/activity_title</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    
     <style name="Zoom_Horizontal">
        <item name="android:background">@drawable/btn_style1</item>
        <item name="android:focusable">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">75dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="Zoom_Vertical">
        <item name="android:background">@drawable/btn_style2</item>
        <item name="android:focusable">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">75dp</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    
    
      <style name="Zoom_Center">
        <item name="android:background">@drawable/btn_style3</item>
        <item name="android:focusable">true</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:textSize">35sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    
         <style name="TextAppearance">
        <item name="android:textColor">@color/white_smoke</item>
        <item name="android:textSize">30sp</item>
    </style>
    
         
      <style name="MyDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.8</item>
    </style>
    <!--自定义dialog背景全透明无边框theme -->
    <style name="MyDialog2" parent="android:style/Theme.Dialog">
        <!--背景颜色及和透明程度-->
        <item name="android:windowBackground">@android:color/black</item>
        <!--是否去除标题 -->
        <item name="android:windowNoTitle">true</item>
        <!--是否去除边框-->
        <item name="android:windowFrame">@null</item>
        <!--是否浮现在activity之上-->
        <item name="android:windowIsFloating">true</item>
        <!--是否模糊-->
        <item name="android:backgroundDimEnabled">false</item>
        <!--对话框宽度-->
        <item name="android:layout_width">840dp</item>
        <!--对话框高度-->
        <item name="android:layout_height">480dp</item>
    </style>
    
     <style name="selectorDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowFrame">@null</item><!-- 边框 -->
        <item name="android:windowIsFloating">true</item><!-- 是否悬浮在activity上 -->
        <item name="android:windowIsTranslucent">false</item><!-- 半透明 -->
        <item name="android:windowNoTitle">true</item><!-- 无标题 -->
        <item name="android:windowBackground">@drawable/shapecolor</item><!-- 内背景 -->
        <item name="android:backgroundDimEnabled">false</item><!-- 模糊 -->
        <item name="android:backgroundDimAmount">0.8</item><!-- 灰度 -->
        <item name="android:windowContentOverlay">@null</item><!-- 对话框是否有遮盖 -->
        <item name="android:colorBackgroundCacheHint">@null</item><!-- 背景缓存颜色 -->
    </style>
    
   
    
</resources>