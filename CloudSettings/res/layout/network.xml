<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@drawable/bg2"
                android:paddingBottom="@dimen/activity_vertical_margin"
                tools:context="com.example.networkdetectool.NetWorkActivity">
    <!-- 200dp->300px;88dp->132px;40sp->60px -->
    <TextView
        android:id="@+id/NetCheck"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/testnet_left_right"
        android:layout_marginRight="@dimen/testnet_left_right"
        android:layout_marginTop="@dimen/testnet_top"
        android:text="@string/net_check"
        android:textSize="@dimen/testnet_textsize_40"
        android:textColor="#fefefe"/>
    <RelativeLayout
        android:layout_height="match_parent"
        android:layout_width="@dimen/testnet_width2"
        android:id="@+id/all"
        android:layout_below="@+id/NetCheck"
        android:layout_alignParentLeft="true">
	
        <ProgressBar
            android:id="@+id/progressBarLarge"
            style="?android:attr/progressBarStyleLarge"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:visibility="gone"/>
    
        <!-- 200dp->300px;14.67dp->22px;20sp->30px -->
        <TextView
            android:id="@+id/textView1"
            android:layout_width="@dimen/testnet_width"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="@dimen/testnet_left_right"
            android:layout_marginTop="@dimen/testnet_top2"
            android:textColor="#a9a9a9"
            android:text="@string/networktip"
            android:textSize="@dimen/testnet_textsize_20" />

        <RelativeLayout
            android:id="@+id/contains"
            android:layout_below="@+id/textView1"
            android:layout_width="@dimen/testnet_width3"
            android:layout_height="@dimen/testnet_height9"
            android:layout_marginLeft="@dimen/testnet_left2"
            android:layout_marginRight="@dimen/testnet_left_right">

            <!-- 25.33sp->38px 120px=>63.33dp->95px-->
            <TextView
                android:id="@+id/textView2"
                android:layout_width="@dimen/testnet_width4"
                android:layout_height="wrap_content"
                android:text="@string/nethardware"
                android:textSize="@dimen/testnet_textsize_25.33"
                android:textColor="#ffffff"
                android:alpha="0.5"
                android:layout_alignTop="@+id/textView3"
                android:layout_alignStart="@+id/textView5"/>


            <TextView
                android:id="@+id/textView3"
                android:layout_width="@dimen/testnet_width10"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/textView2"
                android:layout_marginLeft="@dimen/testnet_left"
                android:layout_marginTop="@dimen/testnet_top3"
                android:text="@string/listcontroltip"
                android:textSize="@dimen/testnet_textsize_18.66"
                android:textColor="#ffffff"
                android:alpha="0.5"/>
            <!--25.33sp->38px-->
            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_alignParentTop="true"
                android:layout_marginRight="@dimen/testnet_right"
                android:layout_marginTop="@dimen/testnet_top3"
                android:text="@string/untested"
                android:textColor="#ffffff"
                android:textSize="@dimen/testnet_textsize_25.33"
                android:alpha="0.5"/>
            <!-- 20dp->35px -->
            <LinearLayout
                android:id="@+id/line1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView3"
                android:layout_marginTop="@dimen/testnet_top4"
                android:layout_marginBottom="@dimen/testnet_top4"
                android:background="@drawable/line"
                android:orientation="horizontal">
            </LinearLayout>
            <!-- #FFA7A7A7 ->  #ffffff-->
            <TextView
                android:id="@+id/textView5"
                android:layout_width="@dimen/testnet_width4"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/testnet_left3"
                android:layout_below="@+id/line1"
                android:text="@string/netconnect"
                android:textSize="@dimen/testnet_textsize_25.33"
                android:alpha="0.5"
                android:textColor="#ffffff"/>

            <TextView
                android:id="@+id/textView6"
                android:layout_width="@dimen/testnet_width10"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/textView5"
                android:layout_below="@+id/line1"
                android:layout_marginLeft="@dimen/testnet_left"
                android:alpha="0.5"
                android:text="@string/networktip2"
                android:textSize="@dimen/testnet_textsize_18.66"
                android:textColor="#ffffff"/>

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/testnet_right"
                android:layout_below="@+id/line1"
                android:alpha="0.5"
                android:text="@string/untested"
                android:textColor="#ffffff"
                android:textSize="@dimen/testnet_textsize_25.33" />

            <LinearLayout
                android:id="@+id/line2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView6"
                android:layout_marginTop="@dimen/testnet_top4"
                android:layout_marginBottom="@dimen/testnet_top4"
                android:background="@drawable/line"
                android:orientation="horizontal">
            </LinearLayout>
     
            <TextView
                android:id="@+id/textView8"
                android:layout_width="@dimen/testnet_width4"
                android:layout_height="wrap_content"
                android:text="@string/networktip3"
                android:textSize="@dimen/testnet_textsize_25.33"
                android:textColor="#ffffff"
                android:layout_below="@+id/line2"
                android:alpha="0.5"
                android:layout_marginLeft="@dimen/testnet_left3"/>

            <TextView
                android:id="@+id/textView9"
                android:layout_width="@dimen/testnet_width10"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/textView8"
                android:layout_marginLeft="@dimen/testnet_left"
                android:layout_below="@+id/line2"
                android:alpha="0.5"
                android:text="@string/networktip4"
                android:textSize="@dimen/testnet_textsize_18.66"
                android:textColor="#ffffff"/>
     
            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/testnet_right"
                android:layout_below="@+id/line2"
                android:text="@string/untested"
                android:alpha="0.5"
                android:textColor="#ffffff"
                android:textSize="@dimen/testnet_textsize_25.33" />

            <LinearLayout
                android:id="@+id/line3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView9"
                android:layout_marginTop="@dimen/testnet_top4"
                android:layout_marginBottom="@dimen/testnet_top4"
                android:background="@drawable/line"
                android:orientation="horizontal">
            </LinearLayout>
     
            <TextView
                android:id="@+id/textView11"
                android:layout_width="@dimen/testnet_width4"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/testnet_left3"
                android:layout_below="@+id/line3"
                android:text="@string/dnstip"
                android:textColor="#ffffff"
                android:alpha="0.5"
                android:textSize="@dimen/testnet_textsize_25.33" />

            <!--18.66sp->28px-->
            <TextView
                android:id="@+id/textView12"
                android:layout_width="@dimen/testnet_width10"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/textView11"
                android:layout_marginLeft="@dimen/testnet_left"
                android:layout_below="@+id/line3"
                android:text="@string/networktip5"
                android:textSize="@dimen/testnet_textsize_18.66"
                android:alpha="0.5"
                android:textColor="#ffffff"/>
     
            <TextView
                android:id="@+id/textView13"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/testnet_right"
                android:layout_below="@+id/line3"
                android:alpha="0.5"
                android:text="@string/untested"
                android:textColor="#ffffff"
                android:textSize="@dimen/testnet_textsize_25.33" />

            <LinearLayout
                android:id="@+id/line4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView12"
                android:layout_marginTop="@dimen/testnet_top4"
                android:layout_marginBottom="@dimen/testnet_top4"
                android:background="@drawable/line"
                android:orientation="horizontal">
            </LinearLayout>
     
            <TextView
                android:id="@+id/textView14"
                android:layout_width="@dimen/testnet_width4"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/testnet_left3"
                android:layout_below="@+id/line4"
                android:alpha="0.5"
                android:text="@string/host"
                android:textSize="@dimen/testnet_textsize_25.33"
                android:textColor="#ffffff"/>
    
            <TextView
                android:id="@+id/textView15"
                android:layout_width="@dimen/testnet_width10"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/textView14"
                android:layout_below="@+id/line4"
                android:layout_marginLeft="@dimen/testnet_left"
                android:text="@string/networktip6"
                android:alpha="0.5"
                android:textSize="@dimen/testnet_textsize_18.66"
                android:textColor="#ffffff"/>
     
            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@+id/line4"
                android:layout_marginRight="@dimen/testnet_right"
                android:text="@string/untested"
                android:alpha="0.5"
                android:textColor="#ffffff"
                android:textSize="@dimen/testnet_textsize_25.33" />

            <LinearLayout
                android:id="@+id/line5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/textView15"
                android:layout_marginTop="@dimen/testnet_top4"
                android:layout_marginBottom="@dimen/testnet_top4"
                android:background="@drawable/line"
                android:orientation="horizontal">
            </LinearLayout>

        </RelativeLayout>

        <Button
            android:id="@+id/buttonOK"
            android:layout_width="@dimen/testnet_width6"
            android:layout_height="@dimen/testnet_height2"
            android:background="@drawable/testnetbutton_focus"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/contains"
            android:layout_marginTop="@dimen/testnet_top5"
            android:textSize="@dimen/testnet_textsize_25"
            android:text="@string/totaltest" 
            android:textColor="@color/white"/>

        <ProgressBar
            android:id="@+id/ProgressBar01"
            style="?android:attr/progressBarStyleHorizontal"
            android:progressDrawable="@drawable/progressbar"
            android:layout_width="@dimen/testnet_width6"
            android:layout_height="@dimen/testnet_height2"
            android:layout_marginTop="@dimen/testnet_right"
            android:layout_centerHorizontal="true"
            android:layout_below="@+id/contains"
            android:max="5"
            android:visibility="gone"/>

    </RelativeLayout>

    <com.hiveview.cloudtv.settings.widget.LauncherFocusViewH
        android:id="@+id/network_focus_view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</RelativeLayout>
