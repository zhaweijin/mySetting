        android:orientation="vertical" >

        <ImageView
            android:id="@+id/home_menu_menu"
            android:layout_width="27.1dp"
            android:layout_height="21.1dp"
            android:layout_marginLeft="59.4dp"
            android:layout_marginTop="69.3dp"
            android:background="@drawable/home_lefttop_menu" />
