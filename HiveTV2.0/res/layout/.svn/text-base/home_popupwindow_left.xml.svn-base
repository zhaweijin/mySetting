        <LinearLayout
            android:id="@+id/home_search"
            android:layout_width="240dp"
            android:layout_height="72dp"
            android:layout_marginTop="30.8dp"
            android:layout_marginLeft="45dp"
            android:focusable="true"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/home_search_image"
                android:layout_width="46.2dp"
                android:layout_height="46.2dp"
                android:layout_gravity="center"
                android:layout_marginLeft="14.4dp"
                android:background="@drawable/home_search" />

            <TextView
                android:id="@+id/home_search_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="26.3dp"
                android:text="@string/sub_navigation_common_search_text"
                android:textColor="#9a9a9a"
                android:textSize="23.76dp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/home_recoder"
